{
  "version": 2,
  
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "/your-dashboard-project/api/index.js"
    },
    {
      "src": "^(.*)$",
      "dest": "/your-dashboard-project/frontend/dist/index.html"
    }
  ]
}
